{
  "name": "deixmas-frontend",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && NODE_ENV=production vite build --mode production",
    "build:dev": "vue-tsc --noEmit && NODE_ENV=development vite build --mode development",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore --fix src",
    "prettier": "prettier --single-quote --write \"**/**/*.{ts,js,css,scss,vue}\""
  },
  "dependencies": {
    "@fontsource/roboto": "^4.5.8",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "axios": "^0.21.4",
    "material-design-icons-iconfont": "^5.0.1",
    "pinia": "^2.1.6",
    "pinia-plugin-persistedstate": "^3.2.0",
    "vue": "^3.2.45",
    "vue-eslint-parser": "^7.11.0",
    "vue-router": "^4.2.4",
    "vuetify": "^3.3.16"
  },
  "devDependencies": {
    "@mdi/font": "^5.9.55",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vue/compiler-sfc": "^3.2.45",
    "@vue/runtime-dom": "^3.2.45",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.49.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^8.7.1",
    "prettier": "2.1.2",
    "sass": "^1.67.0",
    "typescript": "^4.9.5",
    "vite": "^4.4.9",
    "vite-plugin-vuetify": "^1.0.2",
    "vue-tsc": "^1.8.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "es2021": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier",
      "@vue/typescript"
    ],
    "rules": {
      "no-console": 0,
      "error": "off",
      "no-debugger": 0,
      "quotes": [
        "error",
        "single"
      ]
    },
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    }
  },
  "prettier": {
    "singleQuote": true
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "engines": {
    "node": ">=12.14.0",
    "npm": ">=6.13.0"
  }
}
